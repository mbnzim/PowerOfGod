
<!DOCTYPE html>
    <html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>@ViewBag.Title - My ASP.NET Application</title>
        <script src="~/Content/pdfmake.js"></script>
        <link href="~/Content/datatables.min.css" rel="stylesheet" />

        <a href="~/Views/Shared/_Layout.cshtml"></a>
        <link href="~/Content/user/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/Content/user/css/font-awesome.min.css" rel="stylesheet" />
        <link href="~/Content/user/css/themify-icons.css" rel="stylesheet" />
        <link href="~/Content/user/css/style.css" rel="stylesheet" />



    </head>
    <body>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - Developed by Semicolon</p>
            </footer>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
       
    </body>
    </html>

