@model IEnumerable<PowerOfGod.Domain.Entity.Booking.Venue>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutUserLess.cshtml";
}
<br /><br /><br /><br /><br />
<h2>Venue Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
